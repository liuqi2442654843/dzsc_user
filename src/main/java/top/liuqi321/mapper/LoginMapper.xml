<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="top.liuqi321.mapper.LoginMapper">
    <select id="select_user"
            resultType="top.liuqi321.bean.T_MALL_USER_ACCOUNT"
            parameterType="top.liuqi321.bean.T_MALL_USER_ACCOUNT">
            select * from t_mall_user_account where yh_mch = #{yh_mch}
            and yh_mm = #{yh_mm}
    </select>
</mapper>
